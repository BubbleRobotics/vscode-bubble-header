{
  "name": "BR-header",
  "displayName": "BR Header",
  "description": "BR header for VSCode",
  "version": "0.42.7",
  "publisher": "bubble-robotics",
  "icon": "BR_Logo.avif",
  "repository": {
    "type": "git",
    "url": "https://github.com/kube/vscode-42header"
  },
  "bugs": {
    "url": "https://github.com/kube/vscode-42header/issues"
  },
  "homepage": "https://github.com/kube/vscode-42header",
  "license": "MIT",
  "engines": {
    "vscode": "^1.6.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "properties": {
        "BRheader.username": {
          "type": "string",
          "description": "Username to use in BR headers"
        },
        "BRheader.email": {
          "type": "string",
          "description": "Email address to use in BR headers"
        }
      }
    },
    "commands": [
      {
        "command": "BRheader.insertHeader",
        "title": "Insert BR header",
        "when": "editorTextFocus"
      }
    ],
    "keybindings": [
      {
        "command": "BRheader.insertHeader",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^6.0.45",
    "typescript": "^2.0.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "moment": "^2.15.0"
  }
}
